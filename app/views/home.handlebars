<div class="page-wrapper">

    {{> menu}}

    <!-- PAGE CONTAINER-->
    <div class="page-container">

        {{> notification }}

        <!-- MAIN CONTENT-->
        <div class="main-content">
            <div class="section__content section__content--p30">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="au-card recent-report">
                                <div class="au-card-inner">
                                    <table id="example" class="table table-striped table-bordered" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                                <th>About</th>
                                                <th>ISO2</th>
                                                <th>ISO3</th>
                                                <th>Capital</th>
                                                <th>Options</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each country}}
                                            <tr>
                                                <td>{{name}}</td>
                                                <td>{{about}}</td>
                                                <td>{{iso2}}</td>
                                                <td>{{iso3}}</td>
                                                <td>{{capital}}</td>
                                                <td>
                                                    <div class="container">
                                                        <div class="table-data-feature text-center">
                                                            <a href="/dashboard/update?id={{_id}}" class="item"
                                                                data-toggle="tooltip" data-placement="top" title="Edit">
                                                                <i class="zmdi zmdi-edit"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th>Name</th>
                                                <th>About</th>
                                                <th>ISO2</th>
                                                <th>ISO3</th>
                                                <th>Capital</th>
                                                <th>Options</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{> footer}}
                </div>
            </div>
        </div>
        <!-- END MAIN CONTENT-->
        <!-- END PAGE CONTAINER-->
    </div>

</div>